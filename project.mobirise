                "padding-right": "0"
              },
              "@media (max-width: 992px)": {
                "justify-content": "center"
              }
            },
            ".gallery-item": {
              "& when (@flexDirection)": {
                "padding-left": "0",
                "padding-right": "0"
              }
            },
            ".gallery-row, .gallery-item": {
              "padding-left": "0.5rem",
              "padding-right": "0.5rem",
              "@media (max-width: 768px)": {
                "padding-left": "0rem",
                "padding-right": "0rem"
              }
            },
            ".gallery-item, .gallery-row": {
              "&:nth-last-child(1)": {
                "padding-bottom": "0"
              }
            },
            "& when not (@paddingCard)": {
              ".gallery-item, .gallery-row": {
                "padding": "0!important"
              }
            },
